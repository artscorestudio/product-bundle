{
	"name" : "artscorestudio/product-bundle",
	"license" : "MIT",
	"type" : "library",
	"description" : "Product Bundle is a Symfony 2/3 bundle for create and manage products in your Symfony 2/3 application. This package is a part of Artscore Studio Framework.",
	"homepage" : "https://github.com/artscorestudio/product-bundle",
	"authors" : [{
		"name" : "Nicolas Claverie",
		"email" : "info@artscore-studio.fr"
	}, {
		"name" : "Artscore Studio",
		"homepage" : "http://www.artscore-studio.fr"
	}
	],
	"require" : {
		"php" : ">=5.3.9",
		
		"symfony/framework-bundle": "~2.8|~3.0",
        "symfony/expression-language": "~2.8|~3.0",
        "symfony/browser-kit": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/event-dispatcher": "~2.8|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/assetic-bundle": "^2.7",
        
        "doctrine/orm": "~2.4|^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "~1.0|^1.2",
        
        "twig/twig": "~1.10",
        
		"artscorestudio/core-bundle": "~1.0@dev"
	},
	"require-dev": {
	    "artscorestudio/layout-bundle": "~1.0@dev"
	},
	"autoload" : {
		"psr-4" : {
			"ASF\\ProductBundle\\" : ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"minimum-stability" : "dev"
}
