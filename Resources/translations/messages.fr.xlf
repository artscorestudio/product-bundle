            </trans-unit>
            <trans-unit id="asf.product.created_at">
                <source>asf.product.created_at</source>
                <target>Créé le</target>
            </trans-unit>
            <trans-unit id="asf.product.updated_at">
                <source>asf.product.updated_at</source>
                <target>Mis à jour le</target>
            </trans-unit>
            <trans-unit id="asf.product.deleted_at">
                <source>asf.product.deleted_at</source>
                <target>Supprimé le</target>
            </trans-unit>
            
