            	<source>asf.product.msg.delete.confirm</source>
            	<target>Êtes-vous sûr de vouloir supprimer %name% ?</target>
            </trans-unit>
            
            <trans-unit id="asf.product.msg.list.no_category">
            	<source>asf.product.msg.list.no_category</source>
            	<target>Acune catégorie.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.list.no_product">
            	<source>asf.product.msg.list.no_product</source>
            	<target>Aucun produit.</target>
            </trans-unit>
