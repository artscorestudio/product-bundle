            	<source>asf.product.msg.error.category_not_found</source>
            	<target>Catégorie introuvable.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.error.category_already_exists">
            	<source>asf.product.msg.error.category_already_exists</source>
            	<target>Une catégorie portant ce nom existe déjà.</target>
