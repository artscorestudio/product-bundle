            <trans-unit id="asf.product.default_value.product_name">
                <source>asf.product.default_value.product_name</source>
                <target>Nouveau produit</target>
            </trans-unit>
            <trans-unit id="asf.product.default_value.category_name">
                <source>asf.product.default_value.category_name</source>
