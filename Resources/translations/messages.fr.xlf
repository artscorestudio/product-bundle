            <trans-unit id="asf.product.msg.success.product_updated">
            	<source>asf.product.msg.success.product_updated</source>
            	<target>Le produit "%name%" a été mis à jour avec succès.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.success.product_created">
            	<source>asf.product.msg.success.product_created</source>
            	<target>Le produit "%name%" a été créé avec succès.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.success.product_deleted">
            	<source>asf.product.msg.success.product_deleted</source>
            	<target>Le produit "%name%" a été supprimé avec succès.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.error.product_not_found">
            	<source>asf.product.msg.error.product_not_found</source>
            	<target>Produit introuvable.</target>
            </trans-unit>
            <trans-unit id="asf.product.msg.error.product_already_exists">
            	<source>asf.product.msg.error.product_already_exists</source>
            	<target>Un produit portant ce nom existe déjà.</target>
            </trans-unit>
            
