            </trans-unit>
            
            <trans-unit id="asf.product.form.search_product">
                <source>asf.product.form.search_product</source>
                <target>Rechercher un produit</target>
            </trans-unit>
            <trans-unit id="asf.product.form.search_category">
                <source>asf.product.form.search_category</source>
                <target>Rechercher une catégorie</target>
            </trans-unit>
            <trans-unit id="asf.product.form.search_brand">
                <source>asf.product.form.search_brand</source>
                <target>Rechercher une marque</target>
            </trans-unit>
            
            <trans-unit id="asf.product.form.choose_a_product">
                <source>asf.product.form.choose_a_product</source>
                <target>Choisir un produit</target>
            </trans-unit>
            <trans-unit id="asf.product.form.choose_a_category">
                <source>asf.product.form.choose_a_category</source>
                <target>Choisir une catégorie</target>
            </trans-unit>
            <trans-unit id="asf.product.form.choose_a_brand">
                <source>asf.product.form.choose_a_brand</source>
                <target>Choisir une marque</target>
            </trans-unit>
            
            <trans-unit id="asf.product.form.product_no_result">
            	<source>asf.product.form.product_no_result</source>
            	<target>Aucun produit ne correspond.</target>
            </trans-unit>
            
            <!-- PRODUCT -->
            <trans-unit id="asf.product.state">
                <source>asf.product.state</source>
                <target>État</target>
            </trans-unit>
            <trans-unit id="asf.product.product_name">
                <source>asf.product.product_name</source>
                <target>Nom du produit</target>
            </trans-unit>
            <trans-unit id="asf.product.type">
                <source>asf.product.type</source>
                <target>Type</target>
            </trans-unit>
