            <trans-unit id="asf.product.state.draft">
                <source>asf.product.state.draft</source>
                <target>Brouillon</target>
            </trans-unit>
            <trans-unit id="asf.product.state.waiting">
                <source>asf.product.state.waiting</source>
                <target>En attente</target>
            </trans-unit>
            <trans-unit id="asf.product.state.published">
                <source>asf.product.state.published</source>
